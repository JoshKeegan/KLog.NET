<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0</TargetFrameworks>
        <AssemblyVersion>0.3.0.0</AssemblyVersion>
        <FileVersion>0.3.0.0</FileVersion>
        <Authors>Josh Keegan</Authors>
        <Company />
        <Version>0.3.0</Version>
        <PackageProjectUrl>https://github.com/JoshKeegan/KLog.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/JoshKeegan/KLog.NET</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageLicenseUrl>https://github.com/JoshKeegan/KLog.NET/blob/master/LICENSE</PackageLicenseUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>KISS .NET logging library</Description>
        <Copyright>Copyright © Josh Keegan 2012-2018</Copyright>
        <PackageId>KLog.NET</PackageId>
        <PackageTags>logging log diagnostics logfiles tracing</PackageTags>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="System.Data.SqlClient" Version="4.5.0" />
    </ItemGroup>
</Project>